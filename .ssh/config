IdentityFile ~/.ssh/id_rsa
IdentityFile ~/.ssh/id_rlc-aws-master-key_pem

host *amazonaws.com
    CheckHostIP no
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host bitbucket.org
    # Bitbucket has multiplexing disabled
    ControlMaster no

# RLC STL Office Desktop
Host artanis
	ServerAliveInterval 300

Host *
    ControlPath ~/.ssh/mux-%r@%h:%p
    ControlMaster auto
    ControlPersist 30s
